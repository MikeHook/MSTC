          <DateFolder id="1912" parentID="1894" level="5" writerID="0" creatorID="2" nodeType="1125" template="1116" sortOrder="6" createDate="2011-08-30T09:55:40" updateDate="2012-06-29T11:51:35" nodeName="30" urlName="30" writerName="Administrator" creatorName="stevemac" path="-1,1048,1127,1344,1894,1912" isDoc="">
            <BlogPost id="1911" parentID="1912" level="6" writerID="0" creatorID="2" nodeType="1124" template="1118" sortOrder="1" createDate="2011-08-30T09:53:57" updateDate="2012-06-29T11:51:36" nodeName="Mid Sussex Tri Victorious" urlName="mid-sussex-tri-victorious" writerName="Administrator" creatorName="stevemac" path="-1,1048,1127,1344,1894,1912,1911" isDoc="">
              <closeComments>0</closeComments>
