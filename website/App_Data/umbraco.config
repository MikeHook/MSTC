          <DateFolder id="1623" parentID="1613" level="5" writerID="0" creatorID="2" nodeType="1125" template="1116" sortOrder="3" createDate="2011-05-11T09:10:09" updateDate="2012-06-29T11:51:23" nodeName="11" urlName="11" writerName="Administrator" creatorName="stevemac" path="-1,1048,1127,1344,1613,1623" isDoc="">
            <BlogPost id="1622" parentID="1623" level="6" writerID="0" creatorID="2" nodeType="1124" template="1118" sortOrder="1" createDate="2011-05-11T09:03:42" updateDate="2012-06-29T11:51:24" nodeName="Isle of Wight Cycle Tour" urlName="isle-of-wight-cycle-tour" writerName="Administrator" creatorName="stevemac" path="-1,1048,1127,1344,1613,1623,1622" isDoc="">
              <closeComments>0</closeComments>
