        <DateFolder id="2679" parentID="2678" level="4" writerID="2" creatorID="2" nodeType="1125" template="1116" sortOrder="1" createDate="2013-01-02T13:18:09" updateDate="2013-01-02T13:18:09" nodeName="1" urlName="1" writerName="stevemac" creatorName="stevemac" path="-1,1048,1127,2678,2679" isDoc="">
