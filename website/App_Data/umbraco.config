            <BlogPost id="1528" parentID="1529" level="6" writerID="0" creatorID="2" nodeType="1124" template="1118" sortOrder="1" createDate="2011-04-18T22:11:10" updateDate="2012-06-29T11:51:19" nodeName="BAR  Race 1 Cambridge Duathlon" urlName="bar-race-1-cambridge-duathlon" writerName="Administrator" creatorName="stevemac" path="-1,1048,1127,1344,1501,1529,1528" isDoc="">
