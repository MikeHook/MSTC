            <BlogPost id="1904" parentID="1906" level="6" writerID="0" creatorID="12" nodeType="1124" template="1118" sortOrder="1" createDate="2011-08-13T15:27:38" updateDate="2012-06-29T11:51:34" nodeName="Dextro Energy London Tri - 7th Aug 2011" urlName="dextro-energy-london-tri-7th-aug-2011" writerName="Administrator" creatorName="Sharon" path="-1,1048,1127,1344,1894,1906,1904" isDoc="">
