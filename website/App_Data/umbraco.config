            <BlogPost id="2232" parentID="2234" level="6" writerID="0" creatorID="12" nodeType="1124" template="1118" sortOrder="1" createDate="2011-09-07T06:19:43" updateDate="2012-06-29T11:51:36" nodeName="Bar Race Middle Dist 28th Aug 2011" urlName="bar-race-middle-dist-28th-aug-2011" writerName="Administrator" creatorName="Sharon" path="-1,1048,1127,1344,2233,2234,2232" isDoc="">
