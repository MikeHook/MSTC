            <BlogPost id="1841" parentID="1821" level="6" writerID="0" creatorID="12" nodeType="1124" template="1118" sortOrder="2" createDate="2011-07-07T22:31:31" updateDate="2012-06-29T11:51:31" nodeName="Dorney Lake WomenOnly Triathlon - 2nd July 2011" urlName="dorney-lake-womenonly-triathlon-2nd-july-2011" writerName="Administrator" creatorName="Sharon" path="-1,1048,1127,1344,1814,1821,1841" isDoc="">
