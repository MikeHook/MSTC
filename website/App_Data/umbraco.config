    <JuniorsHome id="2972" parentID="1048" level="2" writerID="0" creatorID="0" nodeType="2970" template="2971" sortOrder="15" createDate="2014-02-21T11:19:38" updateDate="2014-02-21T12:34:11" nodeName="Mid Sussex Triathlon Juniors" urlName="mid-sussex-triathlon-juniors" writerName="Administrator" creatorName="Administrator" path="-1,1048,2972" isDoc="">
