            <BlogPost id="2841" parentID="2840" level="6" writerID="2" creatorID="2" nodeType="1124" template="1118" sortOrder="2" createDate="2013-09-24T15:26:35" updateDate="2013-09-24T15:47:16" nodeName="ITU World Triathlon Champs" urlName="itu-world-triathlon-champs" writerName="stevemac" creatorName="stevemac" path="-1,1048,1127,2678,2816,2840,2841" isDoc="">
