            <BlogPost id="2447" parentID="2449" level="6" writerID="0" creatorID="2" nodeType="1124" template="1118" sortOrder="1" createDate="2012-06-01T10:12:48" updateDate="2012-06-29T11:51:56" nodeName="Brighton Triathlon Race Series" urlName="brighton-triathlon-race-series" writerName="Administrator" creatorName="stevemac" path="-1,1048,1127,2299,2448,2449,2447" isDoc="">
