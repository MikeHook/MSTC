            <BlogPost id="1547" parentID="1550" level="6" writerID="0" creatorID="2" nodeType="1124" template="1118" sortOrder="1" createDate="2011-04-28T18:53:08" updateDate="2012-06-29T11:51:21" nodeName="Mark Jordan and his Tinman Challenge" urlName="mark-jordan-and-his-tinman-challenge" writerName="Administrator" creatorName="stevemac" path="-1,1048,1127,1344,1501,1550,1547" isDoc="">
