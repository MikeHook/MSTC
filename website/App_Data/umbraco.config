            <BlogPost id="2524" parentID="2525" level="6" writerID="2" creatorID="2" nodeType="1124" template="1118" sortOrder="1" createDate="2012-08-13T13:33:28" updateDate="2012-08-13T13:42:00" nodeName="Southern Counties 100 mile time trial" urlName="southern-counties-100-mile-time-trial" writerName="stevemac" creatorName="stevemac" path="-1,1048,1127,2299,2517,2525,2524" isDoc="">
