            <BlogPost id="2499" parentID="2496" level="6" writerID="2" creatorID="2" nodeType="1124" template="1118" sortOrder="2" createDate="2012-07-12T15:01:11" updateDate="2012-07-12T15:06:12" nodeName="Sussex Triathlon - Sprint distance" urlName="sussex-triathlon-sprint-distance" writerName="stevemac" creatorName="stevemac" path="-1,1048,1127,2299,2489,2496,2499" isDoc="">
