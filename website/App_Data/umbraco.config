            <BlogPost id="1542" parentID="1545" level="6" writerID="0" creatorID="3" nodeType="1124" template="1118" sortOrder="1" createDate="2011-04-27T11:56:49" updateDate="2012-06-29T11:51:20" nodeName="4 Athletes qualify for World Duathlon Champs" urlName="4-athletes-qualify-for-world-duathlon-champs" writerName="Administrator" creatorName="rachelb" path="-1,1048,1127,1344,1501,1545,1542" isDoc="">
