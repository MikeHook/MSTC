            <BlogPost id="2347" parentID="2892" level="6" writerID="15" creatorID="2" nodeType="1124" template="1118" sortOrder="1" createDate="2012-02-15T11:11:04" updateDate="2013-12-08T21:57:35" nodeName="Snow, Slips and Cyclo" urlName="snow,-slips-and-cyclo" writerName="MikeHook" creatorName="stevemac" path="-1,1048,1127,2299,2338,2892,2347" isDoc="">
