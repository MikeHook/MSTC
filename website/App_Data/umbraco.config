            <BlogPost id="2677" parentID="2680" level="6" writerID="2" creatorID="2" nodeType="1124" template="1118" sortOrder="1" createDate="2013-01-02T13:14:35" updateDate="2013-01-02T13:19:02" nodeName="Portsmouth Coastal Marathon 23rd December (BAR Race)" urlName="portsmouth-coastal-marathon-23rd-december-(bar-race)" writerName="stevemac" creatorName="stevemac" path="-1,1048,1127,2678,2679,2680,2677" isDoc="">
              <closeComments>0</closeComments>
